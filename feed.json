---
layout: null
---

[
{% for post in site.posts %}
    {
        "title":   "{{ post.title | xml_escape }}",
        "url":     "{{ post.url | prepend: site.baseurl | prepend: site.url }}",
        "date":    "{{ post.date | date_to_xmlschema }}",
        "content": "{{ post.content | strip_html }}";
    }{% if forloop.last == false %},{% endif %}
{% endfor %}
]